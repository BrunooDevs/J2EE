<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>DoctorController</servlet-name>
        <servlet-class>sistema.controller.DoctorController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadController</servlet-name>
        <servlet-class>sistema.controller.UploadController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NoticiasController</servlet-name>
        <servlet-class>sistema.controller.NoticiasController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportsController</servlet-name>
        <servlet-class>sistema.controller.ReportsController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminController</servlet-name>
        <servlet-class>sistema.controller.AdminController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportsControllerAdmin</servlet-name>
        <servlet-class>sistema.controller.ReportsControllerAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadServlet</servlet-name>
        <servlet-class>sistema.controller.UploadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DoctorController</servlet-name>
        <url-pattern>/DoctorController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadController</servlet-name>
        <url-pattern>/UploadController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NoticiasController</servlet-name>
        <url-pattern>/NoticiasController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportsController</servlet-name>
        <url-pattern>/ReportsController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminController</servlet-name>
        <url-pattern>/AdminController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportsControllerAdmin</servlet-name>
        <url-pattern>/ReportsControllerAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadServlet</servlet-name>
        <url-pattern>/UploadServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
